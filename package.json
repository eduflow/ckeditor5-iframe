{
  "name": "ckeditor5-iframe",
  "version": "29.2.0",
  "description": "iframe feature for CKEditor 5.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-feature",
    "ckeditor5-plugin",
    "ckeditor5-iframe"
  ],
  "dependencies": {
    "@ckeditor/ckeditor5-clipboard": "^29.2.0",
    "@ckeditor/ckeditor5-core": "^29.2.0",
    "@ckeditor/ckeditor5-engine": "^29.2.0",
    "@ckeditor/ckeditor5-inspector": "^2.2.2",
    "@ckeditor/ckeditor5-ui": "^29.2.0",
    "@ckeditor/ckeditor5-undo": "^29.2.0",
    "@ckeditor/ckeditor5-utils": "^29.2.0",
    "@ckeditor/ckeditor5-widget": "^29.2.0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-basic-styles": "^29.2.0",
    "@ckeditor/ckeditor5-clipboard": "^29.2.0",
    "@ckeditor/ckeditor5-core": "^29.2.0",
    "@ckeditor/ckeditor5-dev-tests": "^24.4.2",
    "@ckeditor/ckeditor5-dev-utils": "^24.0.0",
    "@ckeditor/ckeditor5-editor-classic": "^29.2.0",
    "@ckeditor/ckeditor5-editor-inline": "^29.2.0",
    "@ckeditor/ckeditor5-engine": "^29.2.0",
    "@ckeditor/ckeditor5-essentials": "^29.2.0",
    "@ckeditor/ckeditor5-media-embed": "^29.2.0",
    "@ckeditor/ckeditor5-paragraph": "^29.2.0",
    "@ckeditor/ckeditor5-table": "^29.2.0",
    "@ckeditor/ckeditor5-theme-lark": "^29.2.0",
    "@ckeditor/ckeditor5-ui": "^29.2.0",
    "@ckeditor/ckeditor5-widget": "^29.2.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.1.0",
    "eslint-config-ckeditor5": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "mini-css-extract-plugin": "^1.6.0",
    "raw-loader": "^4.0.2",
    "sanitize-html": "^2.1.0",
    "stylelint": "^13.5.0",
    "stylelint-config-ckeditor5": "^2.0.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.7.1"
  },
  "author": "Eduflow team",
  "license": "MIT",
  "bugs": "https://github.com/eduflow/ckeditor5-iframe/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eduflow/ckeditor5-iframe.git"
  },
  "files": [
    "lang",
    "src",
    "theme"
  ],
  "scripts": {
    "lint": "eslint --quiet src/**/*.js",
    "lint:fix": "eslint --quiet src/**/*.js --fix",
    "stylelint": "stylelint --quiet --allow-empty-input 'theme/**/*.css' 'docs/**/*.css'",
    "test": "node node_modules/@ckeditor/ckeditor5-dev-tests/bin/test.js",
    "start": "node node_modules/.bin/webpack serve --mode development",
    "dll:build": "webpack"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ],
    "**/*.css": [
      "stylelint --quiet --allow-empty-input"
    ]
  },
  "eslintIgnore": [
    "node_modules/**",
    "packages/*/node_modules/**",
    "packages/*/build/**",
    "packages/*/src/lib/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
